{
  "name": "mgs-movies-service",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --watch ./src --ignore ./**/*.test.ts --exec babel-node ./src/server.ts --extensions .ts",
    "dev:client": "cross-env NODE_ENV=development webpack serve --config webpack.config.babel.ts",
    "build": "cross-env NODE_ENV=production DEPLOYMENT_ENV=live webpack --config webpack.config.babel.ts",
    "prod": "node ./build/server.js",
    "lint:js": "eslint . --ext ts,js,tsx,jsx",
    "lint:scss": "stylelint \"**/*.{ts,js,tsx,jsx}\"",
    "lint": "npm run lint:js; npm run lint:scss;",
    "test": "jest --coverage --runInBand --verbose --forceExit",
    "test:watch": "jest --coverage --watch --runInBand",
    "migrate": "sequelize db:migrate",
    "types:generate": "babel-node ./schema.ts --extensions .ts && graphql-codegen --config ./codegen-backend.yml && graphql-codegen --config ./codegen-frontend.yml && rm -rf ./schema.graphql"
  },
  "repository": {
    "type": "git",
